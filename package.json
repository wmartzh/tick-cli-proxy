{
  "name": "tick-proxy",
  "version": "1.0.0",
  "description": "A Node.js-based API service built with Fastify and TypeScript, following the features pattern",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "start": "NODE_ENV=production tsx src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "fastify",
    "typescript",
    "api",
    "rest"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "fastify": "^5.2.0",
    "node-cache": "^5.1.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
